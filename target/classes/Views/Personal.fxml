<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="403.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MadBBDD.producto4.controllers.PersonalController">
   <children>
      <VBox alignment="CENTER" layoutX="97.0" layoutY="-1.0" prefHeight="403.0" prefWidth="445.0">
         <children>
            <ImageView fitHeight="108.0" fitWidth="447.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/logo-ec.png" />
               </image>
            </ImageView>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="85.0" prefWidth="447.0" text="Selecciona una opción para gestionar el personal" textAlignment="CENTER">
               <font>
                  <Font name="Microsoft Sans Serif" size="16.0" />
               </font>
            </Label>
            <VBox alignment="BOTTOM_LEFT" prefHeight="114.0" prefWidth="447.0">
               <children>
                  <HBox alignment="TOP_CENTER" prefHeight="59.0" prefWidth="291.0">
                     <children>
                        <Button fx:id="buttonEditar" contentDisplay="BOTTOM" mnemonicParsing="false" text="Editar">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="buttonAñadir" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#handleAñadir" prefHeight="31.0" prefWidth="69.0" text="Añadir">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="buttonEliminar" mnemonicParsing="false" onAction="#handleEliminar" text="Eliminar">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="buttonVerTodo" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#handleVerTodo" text="Ver todo" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="atras" mnemonicParsing="false" prefHeight="0.0" prefWidth="54.0" onAction="#handleAtras" text="Atrás" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
