<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="MadBBDD.producto4.controllers.ProyectosController">
   <children>
      <VBox alignment="CENTER" layoutY="-1.0" prefHeight="403.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="108.0" fitWidth="447.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/logo-ec.png" />
               </image>
            </ImageView>
            <VBox alignment="BOTTOM_LEFT" prefHeight="114.0" prefWidth="447.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="70.0" prefWidth="603.0" text="Selecciona una opción para gestionar los proyectos" textAlignment="CENTER">
                     <font>
                        <Font name="Microsoft Sans Serif" size="16.0" />
                     </font>
                  </Label>
                  <HBox alignment="TOP_CENTER" prefHeight="59.0" prefWidth="291.0">
                     <children>
                        <Button fx:id="buttonEditar" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#handleEditar" prefHeight="36.0" prefWidth="57.0" text="Editar">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="buttonAñadir" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#handleAñadir" prefHeight="35.0" prefWidth="65.0" text="Añadir">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="buttonEliminar" mnemonicParsing="false" onAction="#handleEliminar" prefHeight="33.0" prefWidth="59.0" text="Eliminar">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="buttonVerTodo" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#handleVerTodo" prefHeight="32.0" prefWidth="74.0" text="Ver todo" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="65.0" prefWidth="595.0">
               <children>
                  <Button fx:id="atras" mnemonicParsing="false" onAction="#handleAtras" prefHeight="25.0" prefWidth="266.0" text="Atrás" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
